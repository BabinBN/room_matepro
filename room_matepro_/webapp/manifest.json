{
  "_version": "1.65.0",
  "sap.app": {
    "id": "roomatepro.roommatepro",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.18.2",
      "toolsId": "9f6208a4-2dff-4ddf-b116-3f4d3568505d"
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.138.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "roomatepro.roommatepro.i18n.i18n"
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlAggregation": "pages",
        "controlId": "app",
        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "path": "roomatepro.roommatepro.view",
        "async": true,
        "viewPath": "roomatepro.roommatepro.view"
      },
      "routes": [
        {
          "name": "RouteView",
          "pattern": ":?query:",
          "target": [
            "TargetView"
          ]
        },
        {
          "name":"login",
          "pattern": "",
          "target":[
            "Login"
          ]
        }
      ],
      "targets": {
        "TargetView": {
          "id": "View",
          "name": "View"
        },
        "Login":{
          "viewId": "Login",
          "viewName": "login.sign_in"
        }
      }
    },
    "rootView": {
      "viewName": "roomatepro.roommatepro.view.App",
      "type": "XML",
      "id": "App",
      "async": true
    }
  }
}
