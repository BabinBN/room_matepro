COLUMN TABLE "master_modules" (
  "id" BIGINT GENERATED BY DEFAULT AS IDENTITY,
  "name" VARCHAR(100) NOT NULL,
  "key" VARCHAR(200),
  "description" VARCHAR(200),
  "icon" VARCHAR(200),
  "parent_id" INTEGER,
  "order" INTEGER,
  "sub_title" VARCHAR(200),
  "footer" VARCHAR(200),
  "status" TINYINT,
  PRIMARY KEY ("id")
)